<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="YearView">
        <attr name="text_color" format="color"/>
        <attr name="text_color_weekend" format="color"/>
        <attr name="vertical_spacing" format="integer"/>
        <attr name="horizontal_spacing" format="integer"/>
        <attr name="current_year" format="integer"/>
        <attr name="rows" format="integer"/>
        <attr name="columns" format="integer"/>
        <attr name="margin_below_month_name" format="integer"/>
        <attr name="today_text_color" format="color"/>
        <attr name="today_background_color" format="color"/>
        <attr name="today_background_geometric_style" >
            <enum name="circle" value="1"/>
            <enum name="square" value="2"/>
        </attr>
        <attr name="firstDayOfWeek">
            <enum name="monday" value="1"/>
            <enum name="tuesday" value="2"/>
            <enum name="wednesday" value="3"/>
            <enum name="thursday" value="4"/>
            <enum name="friday" value="5"/>
            <enum name="saturday" value="6"/>
            <enum name="sunday" value="7"/>
        </attr>
        <attr name="selection_color" format="color"/>
        <attr name="title_gravity">
            <enum name="center" value="1" />
            <enum name="left" value="2" />
            <enum name="right" value="3" />
            <enum name="start" value="2" />
            <enum name="end" value="3" />
        </attr>
    </declare-styleable>
</resources>